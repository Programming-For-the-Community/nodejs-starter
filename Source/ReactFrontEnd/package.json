{
  "name": "react-front-end",
  "version": "1.0.0",
  "description": "Public facing page for of react front-end of a word cloud application",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node server.js",
    "start:dev": "npm run build:dev && dotenv -e .env.dev -- node server.js",
