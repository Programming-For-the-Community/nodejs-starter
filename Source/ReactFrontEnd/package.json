{
  "name": "react-front-end",
  "version": "1.0.0",
  "description": "Public facing page for of react front-end of a word cloud application",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node server.js",
    "start:dev": "dotenv -e .env.dev -- npm run build:dev && node server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm install && webpack --mode production",
    "build:dev": "npm install && npm run lint && webpack --mode development",
    "lint": "npx eslint . --fix --quiet"
  },
  "author": "Charlie Hahm",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "babel-loader": "^9.1.3",
    "dotenv-cli": "^7.4.2",
    "eslint": "^7.32.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.6.0",
    "html-webpack-plugin": "^5.6.0",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.0",
    "winston": "^3.13.0"
  }
}
